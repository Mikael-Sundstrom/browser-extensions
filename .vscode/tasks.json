{
  "version": "2.0.0",
  "tasks": [
    {
      "taskName": "Sass Watch",
      "type": "shell",
      "command": "sass",
      "args": [
        "github.com/scss/:github.com/",
        "--no-cache",
        "--sourcemap=none",
        "--watch"
      ],
      "group": "build",
      "isBackground": true,
      "problemMatcher": []
    },
    {
      "taskName": "Typescript Watch",
      "type": "shell",
      "command": "tsc",
      "args": [
        "--watch",
        "ts/script",
        "--outDir",
        "js"
      ],
      "group": "build",
      "isBackground": true,
      "problemMatcher": []
    }
  ]
}
